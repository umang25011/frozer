{
  "short_name": "Frozer",
  "name": "Frozer",
  "version": "1.0.0",
  "manifest_version": 2,
  "description": "Save and Edit Request, Response",
  "icons": {
    "512": "logo512.png"
  },
  "browser_action": {
    "default_icon": "logo512.png",
    "default_popup": "index.html"
  },
  "background": {
    "persistent": true,
    "scripts": ["../src/background.js"]
  },
  "permissions": ["webRequest", "webRequestBlocking", "tabs", "<all_urls>"]
}
